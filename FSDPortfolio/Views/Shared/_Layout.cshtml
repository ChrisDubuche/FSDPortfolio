<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="robots" content="all,follow">
    <meta name="googlebot" content="index,follow,snippet,archive">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content=".NET developer looking for a challenging role where I can generate significant value." />
    <meta name="keywords" content="HTML, CSS, JavaScript">
 
    <!-- Favicon -->
    <link rel="icon" type="image/png" href="~/img/favicon.png" sizes="16x16">  
    <title>Christian Yves Dubuche | Full Stack Developer</title>

    <!-- Bootstrap and Font Awesome css -->
    <link href="~/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/css/font-awesome.css" rel="stylesheet">
    <link href="~/css/fonts/font-mfizz.css" rel="stylesheet" />

    <!-- Theme stylesheet -->
    <link href="~/css/style.default.css" rel="stylesheet" id="theme-stylesheet">

    <!-- owl carousel css -->

    <link href="~/css/owl.carousel.css" rel="stylesheet">
    <link href="~/css/owl.theme.css" rel="stylesheet">

    <!-- CSS Animations -->
    <link href="~/css/animate.css" rel="stylesheet">

    <link href='http://fonts.googleapis.com/css?family=Roboto+Slab:400,700,100%7CRoboto:400,700,300,100' rel='stylesheet' type='text/css'>

    <link href="~/Scripts/SyntaxHighlighter/styles/shCore.css" rel="stylesheet" />
    <link href="~/Scripts/SyntaxHighlighter/styles/shThemeDefault.css" rel="stylesheet" />
    

    <!-- Custom stylesheet - for your changes -->
    <link href="~/css/custom.css" rel="stylesheet">

    @Scripts.Render("~/bundles/modernizr")

</head>
<body data-spy="scroll" data-target="#navigation" data-offset="120">
    <div class="navbar navbar-default navbar-fixed-top" role="navigation" id="navbar">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand scrollTo" href="#intro">Christian Dubuche</a>
            </div>

            <div class="navbar-collapse collapse" id="navigation">

                <ul class="nav navbar-nav navbar-right">
                    <li class="active">
                        <a href="#intro">Intro</a>
                    </li>
                    <li>
                        <a href="#about">About</a>
                    </li>
                    <li>
                        <a href="#services">Services</a>
                    </li>
                    <li>
                        <a href="#testimonials">Testimonials</a>
                    </li>
                    <li>
                        <a href="#portfolio">Portfolio</a>
                    </li>
                    <li>
                        <a href="#Resume">Résumé</a>
                    </li>
                </ul>
            </div>

        </div>
    </div>

    
    <div class="body-content">
        @RenderBody()
        <hr />
        <div class="section" id="footer">
            <div class="container">

                <div class="row">
                    <div class="col-sm-6">

                        <p class="social">
                            <a href="https://www.linkedin.com/in/christiandubuche" target="_blank" class="external linkedin" data-animate-hover="shake" data-animate="fadeInUp"><i class="fa fa-linkedin"></i></a>
                            <a href="https://twitter.com/ChrisDubuche" target="_blank" class="external twitter" data-animate-hover="shake" data-animate="fadeInUp"><i class="fa fa-twitter"></i></a>                            
                            <a href="https://github.com/ChrisDubuche" target="_blank" class="external github" data-animate-hover="shake" data-animate="fadeInUp"><i class="fa fa-github-alt"></i></a>
                            <a href="https://stackoverflow.com/users/8138529" target="_blank" class="external stack-overflow" data-animate-hover="shake" data-animate="fadeInUp"><i class="fa fa-stack-overflow"></i></a>
                            <a href="mailto:cdubuche@outlook.com?Subject=Hello%20Christian" target="_top" data-animate-hover="shake" data-animate="fadeInUp"><i class="fa fa-envelope-open"></i></a>
                        </p>
                    </div>

                    <div class="col-sm-6">
                        <p>&copy; Christian Yves Dubuche 2017</p>
                    </div>

                    <div class="col-sm-12">
                        <p class="template-bootstrapious">Christian Yves Dubuche </p>
                    </div>
                </div>

            </div>

        </div>

    </div>

    <!-- js base -->
    <script src="~/js/jquery-1.11.0.min.js"></script>
   
    <script src="~/js/bootstrap.min.js"></script>

    <!-- for demo purpose -->
    <script src="~/js/jquery.cookie.js"></script>

    <!-- waypoints for scroll spy -->
    <script src="~/js/waypoints.min.js"></script>

    <!-- maps -->
    <!--<script src="https://maps.googleapis.com/maps/api/js?v=3.exp&amp;sensor=false"></script>-->
    <script src="http://maps.google.com/maps/api/js?key=AIzaSyBtATTNZ3l_rGWReleUEACJbFzS6E1yOx4"></script>

    <!-- maps -->
    <script src="~/js/gmaps.js"></script>

    <!-- masonry layout -->
    <script src="~/js/masonry.pkgd.min.js"></script>

    <!-- owl carousel -->
    <script src="~/js/owl.carousel.min.js"></script>

    <!-- jQuery scroll to -->
    <script src="~/js/jquery.scrollTo.min.js"></script>

    <!-- jQuery counter -->
    <script src="~/js/jquery.counterup.min.js"></script>

    <!-- jQuery parallax -->
    <script src="~/js/jquery.parallax-1.1.3.js"></script>

    <!-- main js file -->
    <script src="~/js/front.js"></script>
    <script src="~/Scripts/SyntaxHighlighter/shCore.js"></script>
    <script src="~/Scripts/SyntaxHighlighter/shBrushJScript.js"></script>
    <script src="~/js/mycustom.js"></script>
    <script>SyntaxHighlighter.all();</script>

    @RenderSection("scripts", required: false)
</body>
</html>
